{
  "name": "pkeidel/dbtolaravel",
  "description": "Reads a database and creates the migrations, models, controllers and blades for the tables",
  "keywords": ["laravel5", "dbtolaravel", "db2laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "PKeidel",
      "email": "PKeidel@sags-per-mail.de"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "doctrine/dbal": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "PKeidel\\DBtoLaravel\\": "src/"
    }
  },
  "config": {
      "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
      "laravel": {
          "providers": [
              "PKeidel\\DBtoLaravel\\Providers\\DBtoLaravelServiceProvider"
          ]
      }
  }
}
